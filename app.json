{
  "expo": {
    "name": "Cloudy",
    "slug": "Cloudy",
    "scheme": "cloudy",
    "version": "1.0.0",
    "description": "A mindful space for your daily memories and gratitude.",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "backgroundColor": "#FFF9F0",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFF9F0"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFF9F0"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": true,
      "package": "app.cloudy.journal"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      "@react-native-google-signin/google-signin",
      "expo-notifications",
      "expo-asset",
      "expo-localization"
    ],
    "extra": {
      "eas": {
        "projectId": "76e57197-421f-4273-8947-00b1655fa31a"
      }
    }
  }
}